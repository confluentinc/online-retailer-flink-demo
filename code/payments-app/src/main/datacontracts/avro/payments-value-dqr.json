    "ruleSet": {
        "domainRules": [
            {
                "name": "pci_encrypt",
                "kind": "TRANSFORM",
                "mode": "WRITEREAD",
                "type": "ENCRYPT",
                "tags": ["pci"],
                "params": {
                    "encrypt.kek.name": "pci_encrypt_key",
                    "encrypt.kms.key.id": "arn:aws:kms:us-east-2:550017254839:key/3e6ad20b-efdf-4b7d-a88c-c99894876652",
                    "encrypt.kms.type": "aws-kms"
